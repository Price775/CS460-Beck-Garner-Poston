bgp:
	g++ -std=c++11 symbolTable.cpp node.cpp driver.cpp -o mainDriver
	g++ -std=c++11 symbolTable.cpp node.cpp searchTest.cpp -o searchDriver
	g++ -std=c++11 symbolTable.cpp node.cpp insertTest.cpp -o insertDriver
	g++ -std=c++11 symbolTable.cpp node.cpp push_and_popTest.cpp -o ppDriver
clean:
	rm mainDriver searchDriver insertDriver ppDriver
